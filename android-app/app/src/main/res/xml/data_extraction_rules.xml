<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="cache" />
        <exclude domain="external" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer but exclude cache -->
        <exclude domain="cache" />
    </device-transfer>
</data-extraction-rules>